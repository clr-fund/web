{
  "name": "test",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Tommy Cox <tommy.cox@protonmail.com>",
  "private": true,
  "scripts": {
    "build": "yarn compile && quasar build",
    "compile": "buidler compile",
    "dev": "rm -rf build && buidler compile && quasar dev",
    "ganache": "ganache-cli -d",
    "lint:contracts:prettier": "prettier --write 'test/**/*.{js,ts}' 'contracts/**/*.sol'",
    "lint:contracts:solhint": "buidler check",
    "lint:contracts": "yarn lint:contracts:prettier && yarn lint:contracts:solhint",
    "lint": "yarn lint:contracts",
    "lint:frontend": "yarn lint:frontend:prettier && yarn lint:frontend:eslint",
    "lint:frontend:prettier": "prettier --write 'src/**/*.{js,ts}'",
    "lint:frontend:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "test": "buidler test",
    "web": "quasar dev",
    "deploy": "push-dir --dir=dist/spa --remote=origin --branch=master"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@resolver-engine/core": "^0.3.3",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "@types/sinon-chai": "^3.2.3",
    "axios": "^0.18.1",
    "chai": "^4.2.0",
    "es6-promisify": "^6.0.0",
    "ethers": "^4.0.44",
    "maci": "0.0.1",
    "p-timeout": "^2.0.1",
    "quasar": "^1.0.0",
    "sass-loader": "^8.0.2",
    "ts-generator": "^0.0.8",
    "ts-node": "^8.6.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.1.2",
    "@nomiclabs/buidler-ethers": "^1.1.2",
    "@nomiclabs/buidler-solhint": "^1.1.2",
    "@nomiclabs/buidler-waffle": "^1.1.2",
    "@openzeppelin/contracts": "^3.0.0-beta.0",
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-typescript": "^1.0.0-beta.2",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "ethereum-waffle": "^2.3.2",
    "ganache-cli": "^6.9.0",
    "npm-check-updates": "^4.0.1",
    "prettier-plugin-solidity": "^1.0.0-alpha.38",
    "push-dir": "^0.4.1",
    "solhint-plugin-prettier": "^0.0.4",
    "typescript": "^3.7.5",
    "vue-cli-plugin-ethers": "~0.1.4"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
